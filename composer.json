{
    "name": "cloakings/cloakings-magicchecker",
    "description": "Cloakings MagicChecker Client",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-sockets": "*",
        "cloakings/cloakings-common": "^1.4",
        "gupalo/json": "^1.0",
        "psr/log": "^3.0",
        "symfony/http-client": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Cloakings\\CloakingsMagicChecker\\": "src/CloakingsMagicChecker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cloakings\\Tests\\CloakingsMagicChecker\\": "tests/CloakingsMagicChecker/"
        }
    }
}
